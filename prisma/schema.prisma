generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Locales {
  id         Int      @id @default(autoincrement())
  name       String   @unique
  image      String
  slogan     String
  created_at DateTime @default(now())
  x          Float
  y          Float
}
